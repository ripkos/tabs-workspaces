{
	"name": "tab-workspaces",
	"version": "0.0.1",
	"manifest_version": 3,
	"icons": {
		"16": "icon/16.png",
		"32": "icon/32.png",
		"48": "icon/48.png",
		"96": "icon/96.png",
		"128": "icon/128.png"
	},
	"action": {
		"default_popup": "src/main/index.html"
	},
	"background": {
		"scripts": ["src/background/background.ts"]
	},
	"permissions": ["tabs", "storage", "tabHide"],
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost:*; object-src 'self';"
	}
}
